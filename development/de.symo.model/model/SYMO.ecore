<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="symopd" nsURI="http://www.dlr.de/symo/model/2016/" nsPrefix="symo">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="The symopd package is part of the SYMO4PD Ecore model and  includes all the basic elements to describe a technical system. This includes the description of the system structure and the system behavior. In addition, the organization of systems in system component libraries and project repositories."/>
  </eAnnotations>
  <eAnnotations source="http://www.eclipse.org/OCL/Import"/>
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore"/>
  <eClassifiers xsi:type="ecore:EClass" name="Parameter" eSuperTypes="SYMO_Base.ecore#//AParameterDef SYMO_Base.ecore#//IOclValidated">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="A parameter describes a system property, such as the system mass or energy consumption. The parameter can be defined for different system states and can also meet various constraints."/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="NeedsValue"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="NeedsValue" value="Tuple {&#xD;&#xA;&#x9;message : String = 'Parameter: Value is needed if Parameter is source of a Balancing',&#xD;&#xA;&#x9;status : Boolean = Balancing.allInstances()->exists(sources->includes(self)) implies self.value &lt;> null&#xD;&#xA;}.status"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="System" eSuperTypes="SYMO_Base.ecore#//ADataItem">
    <eStructuralFeatures xsi:type="ecore:EReference" name="components" ordered="false"
        upperBound="-1" eType="#//SystemElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SystemElement" eSuperTypes="SYMO_Base.ecore#//AElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
        eType="ecore:EClass SYMO_Base.ecore#//Parameter"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//SystemElement" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="#//Connection" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Connection">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="ecore:EClass SYMO_Base.ecore#//ADataItem"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="ecore:EClass SYMO_Base.ecore#//ADataItem"/>
  </eClassifiers>
</ecore:EPackage>
